<!--pages/location/location.wxml-->
<view class='content'>
  <view class='head'>
    当前省份：{{currentProvince}}    
    城市：{{currentCity}}    
  </view>
  <view wx:for="{{university}}" class='university' bindtap='back'>
    <text bindtap="updateLocation" data-id="{{item.Id}}">{{item.Name}}</text>
  </view>
</view>